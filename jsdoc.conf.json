{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js|jsx|ts|tsx)$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "better-docs/component"
  ],
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/better-docs"
  },
  "templates": {
    "better-docs": {
      "name": "简历生成器文档"
    }
  },
  "tags": {
    "allowUnknownTags": ["component"]
  }
}